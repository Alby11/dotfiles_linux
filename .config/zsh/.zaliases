### NETSTAT ALIASES ###

# Alias for checking open listening ports
# Useful for finding services running on a machine.
alias netlist='netstat -tuln'
# Example: netlist

# Alias for showing all active TCP/UDP connections
# Useful for diagnosing network connections.
alias netall='netstat -atun'
# Example: netall

# Alias for detailed network interface statistics
# Useful to monitor packet transmission and errors.
alias netstatiface='netstat -i'
# Example: netstatiface

# Alias for showing network routing table
# Useful for understanding routing behavior of the system.
alias netroute='netstat -rn'
# Example: netroute

### SS (Socket Stat) ALIASES ###

# Alias for showing all listening sockets
alias ssa="ss -tulnep"

# Alias for displaying all TCP connections (ss is faster than netstat)
# Useful for quickly analyzing active connections.
alias ss_tcp='ss -t -a'
# Example: ss_tcp

# Alias for displaying all UDP connections
# Useful for troubleshooting UDP-based services.
alias ss_udp='ss -u -a'
# Example: ss_udp

# Alias for showing all connections with detailed info (including process)
# Useful for troubleshooting network services and finding which process is using a port.
alias ss_detail='ss -tunap'
# Example: ss_detail

# Alias for showing listening sockets
# Useful for checking services that are accepting connections.
alias ss_listen='ss -ltn'
# Example: ss_listen

### PS (Process Status) ALIASES ###

alias psa='ps aux'

# Alias for displaying the top 10 CPU-consuming processes
# Useful for finding processes consuming too much CPU.
alias pscpu='ps aux --sort=-%cpu | head -n 11'
# Example: pscpu

# Alias for displaying the top 10 memory-consuming processes
# Useful for finding processes consuming too much memory.
alias psmem='ps aux --sort=-%mem | head -n 11'
# Example: psmem

# Alias for displaying all processes in a tree format
# Useful for visualizing parent-child relationships between processes.
alias pstree='ps -ejH'
# Example: pstree

# Alias for displaying processes of a specific user
# Useful for monitoring what processes a particular user is running.
alias psuser='ps -u $USER'
# Example: psuser

### NMAP ALIASES ###

# Alias for performing a quick scan of open ports on a specific host
# Useful for fast discovery of open ports.
alias nmapquick='nmap -T4 -F'
# Example: nmapquick 192.168.1.1

# Alias for scanning all TCP ports on a host
# Useful for a complete scan of services on a host.
alias nmaptcp='nmap -p-'
# Example: nmaptcp 192.168.1.1

# Alias for a ping scan to discover active hosts on a network
# Useful for finding live systems on a network.
alias nmapping='nmap -sn'
# Example: nmapping 192.168.1.0/24

# Alias for scanning open ports with service version detection
# Useful for finding versions of services running on open ports.
alias nmapver='nmap -sV'
# Example: nmapver 192.168.1.1

# Alias for scanning and discovering network vulnerabilities (using default scripts)
# Useful for quickly checking for known vulnerabilities.
alias nmapvuln='nmap --script=vuln'
# Example: nmapvuln 192.168.1.1

