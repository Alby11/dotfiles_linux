### Systemd block
if command -v systemctl &>/dev/null; then

	# Basic systemctl commands
	alias ctl="sudo systemctl"                             # General systemctl (system scope)
	alias ctlusr="systemctl --user"                       # General systemctl (user scope)

	# Daemon reloads
	alias ctldr="sudo systemctl daemon-reload"            # Reload system daemon
	alias ctlusrdr="systemctl --user daemon-reload"        # Reload user daemon

	# Start/Stop/Restart/Reload commands
	alias ctlsp="sudo systemctl stop "                    # Stop system service
	alias ctlusrsp="systemctl --user stop "               # Stop user service
	alias ctlst="sudo systemctl start "                   # Start system service
	alias ctlusrst="systemctl --user start "              # Start user service
	alias ctlrt="sudo systemctl restart "                 # Restart system service
	alias ctlusrrt="systemctl --user restart "            # Restart user service
	alias ctlrl="sudo systemctl reload "                  # Reload system service
	alias ctlusrrl="systemctl --user reload "             # Reload user service

	# Status commands
	alias ctls="sudo systemctl status "                   # Status of system service
	alias ctlusrs="systemctl --user status "              # Status of user service

	# Enable/Disable units
	alias ctle="sudo systemctl enable "                   # Enable system unit
	alias ctlusre="systemctl --user enable "              # Enable user unit
	alias ctld="sudo systemctl disable "                  # Disable system unit
	alias ctlusrd="systemctl --user disable "             # Disable user unit

	# Mask/Unmask units
	alias ctlmask="sudo systemctl mask "                  # Mask system unit
	alias ctlusrmask="systemctl --user mask "             # Mask user unit
	alias ctlunmask="sudo systemctl unmask "              # Unmask system unit
	alias ctlusrunmask="systemctl --user unmask "         # Unmask user unit

	# Failed units and reset status
	alias ctlfailed="sudo systemctl --failed --all "      # List failed system units
	alias ctlusrfailed="systemctl --user --failed --all " # List failed user units
	alias ctlresetfailed="sudo systemctl reset-failed "            # Reset failed system units
	alias ctlusrresetfailed="systemctl --user reset-failed "       # Reset failed user units

	# List units
	alias ctllu="sudo systemctl list-units"               # List all system units
	alias ctlusru="systemctl --user list-units"           # List all user units
	alias ctllus="sudo systemctl list-units --type=service"  # List system services
	alias ctlusrus="systemctl --user list-units --type=service"  # List user services

	# Analyze Systemd critical chain
	alias ctlchain="sudo systemd-analyze critical-chain"  # Show system critical chain
	alias ctlusrchain="systemd-analyze critical-chain --user"  # Show user critical chain

	# Generate graph for visualization (requires Graphviz)
	alias ctlgraph='sudo systemd-analyze dot | dot -Tsvg -o systemd-hierarchy.svg && echo "Generated: systemd-hierarchy.svg"'  # Graph for system units
	alias ctlusrgraph='systemd-analyze dot --user | dot -Tsvg -o user-systemd-hierarchy.svg && echo "Generated: user-systemd-hierarchy.svg"'  # Graph for user units

	# Show startup times
	alias ctlblame="sudo systemd-analyze blame"           # Show system startup times
	alias ctlusrblame="systemd-analyze blame --user"      # Show user startup times

	# List dependencies for units
	alias ctldeps="sudo systemctl list-dependencies "     # List dependencies for a system unit
	alias ctlusrdeps="systemctl --user list-dependencies "  # List dependencies for a user unit

	# Inspect unit properties
	alias ctlshow="sudo systemctl show "                  # Show properties of a system unit
	alias ctlusrshow="systemctl --user show "             # Show properties of a user unit

	# Check Unit Files
	alias ctlcat="sudo systemctl cat "                    # View system unit file
	alias ctlusrcat="systemctl --user cat "               # View user unit file

	# Edit units
	alias ctledit="sudo systemctl edit "                  # Edit system unit file
	alias ctlusredit="systemctl --user edit "             # Edit user unit file

	# Targets (system scope)
	alias ctlts="sudo systemctl list-unit-files --type=target"         # List all system targets
	alias ctltu="sudo systemctl list-units --type=target"             # List active system targets
	alias ctltd="sudo systemctl list-dependencies "                  # Show dependencies for a specific system target
	alias ctltg="sudo systemctl get-default"                          # Get the default system target
	alias ctltsd="sudo systemctl set-default "                        # Set the default system target
	alias ctltiso="sudo systemctl isolate "                           # Start or isolate a specific system target

	# Targets (user scope)
	alias ctlusrts="systemctl --user list-unit-files --type=target"    # List all user targets
	alias ctlusrtu="systemctl --user list-units --type=target"        # List active user targets
	alias ctlusrtd="systemctl --user list-dependencies "             # Show dependencies for a specific user target
	alias ctlusrtg="systemctl --user get-default"                     # Get the default user target
	alias ctlusrtsd="systemctl --user set-default "                   # Set the default user target
	alias ctlusrtiso="systemctl --user isolate "                      # Start or isolate a specific user target

	# Show environment
	alias ctlenv="sudo systemctl show-environment"                # Show system environment
	alias ctlusrenv="systemctl --user show-environment"           # Show user environment

	# Show Systemd version
	alias ctlver="systemctl --version"                    # Show systemd version
fi
## Journalctl
if command -v journalctl &>/dev/null; then

	# Logs for system units
	alias j="sudo journalctl"                         # General journalctl
	alias ju="sudo journalctl -u "                   # Logs for a specific system unit
	alias jf="sudo journalctl --follow"              # Follow logs
	alias jk="sudo journalctl -k"                    # Kernel logs
	alias juc="sudo journalctl --catalog --pager-end --unit "  # Logs with catalog for a specific system unit
	alias jfc="sudo journalctl --follow --catalog --unit "     # Follow logs with catalog for a specific system unit
	alias jfk="sudo journalctl --follow --dmesg"     # Follow kernel logs (dmesg)

	# Logs for user units
	alias jus="journalctl --user"                    # General journalctl (user scope)
	alias jusu="journalctl --user -u "              # Logs for a specific user unit
	alias jusf="journalctl --user --follow"         # Follow logs for user services
	alias jusk="journalctl --user -k"               # Kernel logs (user scope)
	alias jusuc="journalctl --user --catalog --pager-end --unit "  # Logs with catalog for a specific user unit
	alias jusfc="journalctl --user --follow --catalog --unit "     # Follow logs with catalog for a specific user unit

fi
### End of Systemd block

# Check if the command 'ccze' is available
